                        <typeOfPart>N</typeOfPart>
                      </partMetadata>
                      <partTexts>
                        <partText>
                          <mixedText lang="de">pro einzelne Stunde<tab/>Fr. 45</mixedText>
                        </partText>
                        <partText>
                          <mixedText lang="fr">par heure isol&#xE9;e<tab/>Fr. 45</mixedText>
                        </partText>
                      </partTexts>
                    </articleText>
                  </subparts>
                </articleText>
                <articleText>
                  <partMetadata ID="EnumerationItem:21694">
                    <partNumber>b)</partNumber>
                    <typeOfPart>C</typeOfPart>
                  </partMetadata>
                  <partTexts>
                    <partText>
                      <mixedText lang="de">Mitglieder:</mixedText>
                    </partText>
                    <partText>
                      <mixedText lang="fr">membres:</mixedText>
                    </partText>
                  </partTexts>
                  <subparts>
                    <articleText>
                      <partMetadata ID="EnumerationItem:21695">
                        <partNumber>1.</partNumber>
                        <typeOfPart>N</typeOfPart>
                      </partMetadata>
                      <partTexts>
                        <partText>
                          <mixedText lang="de">pro Tag<tab/>Fr. 230</mixedText>
                        </partText>
                        <partText>
                          <mixedText lang="fr">par jour<tab/>Fr. 230</mixedText>
                        </partText>
                      </partTexts>
                    </articleText>
                    <articleText>
                      <partMetadata ID="EnumerationItem:21696">
                        <partNumber>2.</partNumber>
                        <typeOfPart>N</typeOfPart>
                      </partMetadata>
                      <partTexts>
                        <partText>
                          <mixedText lang="de">pro Halbtag<tab/>Fr. 140</mixedText>
                        </partText>
                        <partText>
                          <mixedText lang="fr">par demi-jour<tab/>Fr. 140</mixedText>
                        </partText>
                      </partTexts>
                    </articleText>
